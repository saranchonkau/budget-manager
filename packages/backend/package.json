    "pg": "^8.7.1",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@types/http-errors": "^1.8.1",
    "@types/node": "16.x",
    "chokidar": "^3.5.2",
    "concurrently": "^6.3.0",
    "esbuild": "0.13.7",
    "prettier": "2.4.1",
    "typescript": "4.5.0-dev.20211016"
